                  <MenuItem mnemonicParsing="false" text="Sub-Grupo" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Lançamentos">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu fx:id="mnuSair" mnemonicParsing="false" onAction="#sairAction" text="Opções">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#sairAction" text="Sair" />
               </items></Menu>
        </menus>
      </MenuBar>
